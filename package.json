{
  "name": "balisnaptrip-web-workspace",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.15.4",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test",
    "core-api:dev": "pnpm --filter @bst/core-api dev",
    "core-api:build": "pnpm --filter @bst/core-api build",
    "content-manager:dev": "pnpm --filter @bst/content-manager dev",
    "content-manager:build": "pnpm --filter @bst/content-manager build",
    "backfill:catalog-bridge": "pnpm --filter @bst/core-api backfill:catalog-bridge",
    "backfill:booking-bridge": "pnpm --filter @bst/core-api backfill:booking-bridge",
    "backfill:payment-finance-bridge": "pnpm --filter @bst/core-api backfill:payment-finance-bridge",
    "gate:catalog-bridge": "pnpm --filter @bst/core-api gate:catalog-bridge",
    "gate:booking-bridge": "pnpm --filter @bst/core-api gate:booking-bridge",
    "gate:payment-finance-bridge": "pnpm --filter @bst/core-api gate:payment-finance-bridge",
    "gate:release-candidate-ui": "node ./scripts/release-candidate-ui-gates.mjs",
    "gate:public-web-continuity": "pnpm --filter next-app-template gate:public-web-continuity",
    "deploy:stagging-bst": "node ./scripts/stagging-bst/deploy.mjs",
    "deploy:stagging-bst:build": "node ./scripts/stagging-bst/deploy.mjs --run-install --run-build",
    "deploy:stagging-bst:list": "node ./scripts/stagging-bst/list.mjs",
    "deploy:stagging-bst:rollback": "node ./scripts/stagging-bst/rollback.mjs",
    "deploy:core-api-prod": "node ./scripts/stagging-bst/deploy.mjs --remote-base /home/bonk/backend/core-api-prod",
    "deploy:core-api-prod:build": "node ./scripts/stagging-bst/deploy.mjs --remote-base /home/bonk/backend/core-api-prod --run-install --run-build --build-core-api-only",
    "deploy:core-api-prod:list": "node ./scripts/stagging-bst/list.mjs --remote-base /home/bonk/backend/core-api-prod",
    "deploy:core-api-prod:rollback": "node ./scripts/stagging-bst/rollback.mjs --remote-base /home/bonk/backend/core-api-prod",
    "deploy:content-manager-prod": "node ./scripts/stagging-bst/deploy.mjs --remote-base /home/bonk/backend/content-manager-prod",
    "deploy:content-manager-prod:build": "node ./scripts/stagging-bst/deploy.mjs --remote-base /home/bonk/backend/content-manager-prod --run-install --run-build --build-content-manager-only",
    "deploy:content-manager-prod:list": "node ./scripts/stagging-bst/list.mjs --remote-base /home/bonk/backend/content-manager-prod",
    "deploy:content-manager-prod:rollback": "node ./scripts/stagging-bst/rollback.mjs --remote-base /home/bonk/backend/content-manager-prod",
    "deploy:core-api-prod:status": "node ./scripts/stagging-bst/core-api-runtime.mjs --remote-base /home/bonk/backend/core-api-prod status",
    "deploy:core-api-prod:start": "node ./scripts/stagging-bst/core-api-runtime.mjs --remote-base /home/bonk/backend/core-api-prod start",
    "deploy:core-api-prod:stop": "node ./scripts/stagging-bst/core-api-runtime.mjs --remote-base /home/bonk/backend/core-api-prod stop",
    "deploy:core-api-prod:restart": "node ./scripts/stagging-bst/core-api-runtime.mjs --remote-base /home/bonk/backend/core-api-prod restart",
    "deploy:core-api-prod:redis:status": "node ./scripts/stagging-bst/core-api-redis-runtime.mjs --remote-base /home/bonk/backend/core-api-prod status",
    "deploy:core-api-prod:redis:start": "node ./scripts/stagging-bst/core-api-redis-runtime.mjs --remote-base /home/bonk/backend/core-api-prod start",
    "deploy:core-api-prod:redis:stop": "node ./scripts/stagging-bst/core-api-redis-runtime.mjs --remote-base /home/bonk/backend/core-api-prod stop",
    "deploy:core-api-prod:redis:restart": "node ./scripts/stagging-bst/core-api-redis-runtime.mjs --remote-base /home/bonk/backend/core-api-prod restart",
    "gate:ingest-env-baseline": "node ./scripts/stagging-bst/ingest-env-baseline-gate.mjs"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "turbo": "^2.3.3"
  }
}
