{
  "name": "balisnaptrip-web-workspace",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.15.4",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test",
    "core-api:dev": "pnpm --filter @bst/core-api dev",
    "core-api:build": "pnpm --filter @bst/core-api build",
    "deploy:stagging-bst": "node ./scripts/masterbst/deploy.mjs",
    "deploy:stagging-bst:build": "node ./scripts/masterbst/deploy.mjs --run-install --run-build",
    "deploy:stagging-bst:list": "node ./scripts/masterbst/list.mjs",
    "deploy:stagging-bst:rollback": "node ./scripts/masterbst/rollback.mjs",
    "deploy:core-api-prod": "node ./scripts/masterbst/deploy.mjs --remote-base /home/bonk/backend/core-api-prod",
    "deploy:core-api-prod:build": "node ./scripts/masterbst/deploy.mjs --remote-base /home/bonk/backend/core-api-prod --run-install --run-build --build-core-api-only",
    "deploy:core-api-prod:list": "node ./scripts/masterbst/list.mjs --remote-base /home/bonk/backend/core-api-prod",
    "deploy:core-api-prod:rollback": "node ./scripts/masterbst/rollback.mjs --remote-base /home/bonk/backend/core-api-prod",
    "deploy:core-api-prod:status": "node ./scripts/masterbst/core-api-runtime.mjs --remote-base /home/bonk/backend/core-api-prod status",
    "deploy:core-api-prod:start": "node ./scripts/masterbst/core-api-runtime.mjs --remote-base /home/bonk/backend/core-api-prod start",
    "deploy:core-api-prod:stop": "node ./scripts/masterbst/core-api-runtime.mjs --remote-base /home/bonk/backend/core-api-prod stop",
    "deploy:core-api-prod:restart": "node ./scripts/masterbst/core-api-runtime.mjs --remote-base /home/bonk/backend/core-api-prod restart",
    "deploy:core-api-prod:redis:status": "node ./scripts/masterbst/core-api-redis-runtime.mjs --remote-base /home/bonk/backend/core-api-prod status",
    "deploy:core-api-prod:redis:start": "node ./scripts/masterbst/core-api-redis-runtime.mjs --remote-base /home/bonk/backend/core-api-prod start",
    "deploy:core-api-prod:redis:stop": "node ./scripts/masterbst/core-api-redis-runtime.mjs --remote-base /home/bonk/backend/core-api-prod stop",
    "deploy:core-api-prod:redis:restart": "node ./scripts/masterbst/core-api-redis-runtime.mjs --remote-base /home/bonk/backend/core-api-prod restart",
    "gate:ingest-env-baseline": "node ./scripts/masterbst/ingest-env-baseline-gate.mjs"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "turbo": "^2.3.3"
  }
}
